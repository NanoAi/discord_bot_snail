  "keywords": [],
  "main": "./dist/index.ts",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "deps": "echo --MINOR-- && pnpm exec taze minor -r && echo --MAJOR-- && pnpm exec taze major -r && echo.",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preinstall": "npx only-allow pnpm",
    "prisma": "pnpm exec prisma format && pnpm exec prisma generate && pnpm prisma:fix",
    "prisma:fix": "pnpm exec eslint ./src/database/drizzle.schema.ts --fix",
    "prisma:push": "pnpm exec prisma db push && pnpm prisma:fix",
    "start": "tsx src/index.ts",
    "watch": "tsx watch src/index.ts",
    "v": "node -v"
  },
  "dependencies": {
    "@discordjs/formatters": "^0.6.1",
    "chalk": "^5.6.2",
    "dayjs": "^1.11.18",
    "discord.js": "^14.22.1",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "i18next": "^25.5.2",
    "i18next-fs-backend": "^2.6.0",
    "morgan": "^1.10.1",
    "node-cache": "^5.1.2",
    "pg": "^8.16.3",
    "pino": "^9.9.5",
    "pino-pretty": "^13.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@types/eslint": "^8.56.12",
    "@types/morgan": "^1.9.10",
