  "keywords": [],
  "main": "./dist/index.ts",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preinstall": "npx only-allow pnpm",
    "prisma": "pnpm exec prisma format && pnpm exec prisma generate && pnpm prisma:fix",
    "prisma:fix": "pnpm exec eslint ./src/database/drizzle.schema.ts --fix",
    "prisma:push": "pnpm exec prisma db push && pnpm prisma:fix",
    "start": "nodemon -e ts -w ./src -x pnpm run watch:serve",
    "watch:serve": "tsx src/index.ts",
    "v": "node -v"
  },
  "dependencies": {
    "@types/pg": "^8.11.10",
    "chalk": "^5.3.0",
    "dayjs": "^1.11.13",
    "discord.js": "^14.16.2",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.33.0",
    "i18next": "^23.15.1",
    "i18next-fs-backend": "^2.3.2",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.7",
    "node-cache": "^5.1.2",
    "nodemon": "^3.1.6",
    "ora": "^8.1.0",
    "pg": "^8.13.0",
    "pino": "7.0.0-rc.9",
    "pino-pretty": "^11.2.2",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.10.0",
    "@types/eslint": "^8.56.12",
    "@types/morgan": "^1.9.9",
