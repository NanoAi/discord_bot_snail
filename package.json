    "drizzle-prisma-generator": "^0.1.7",
    "eslint": "^9.10.0",
    "glob": "^10.4.5",
    "prisma": "^5.19.1",
    "taze": "^0.14.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}
